{
  "id": "nl.bochove.knmi",
  "version": "0.1.0",
  "compatibility": ">=5.0.0",
  "sdk": 3,
  "name": {
    "en": "KNMI (Netherlands)",
    "nl": "KNMI (Nederland)"
  },
  "description": {
    "en": "Fetches dutch weatherdata from the KNMI up to 300 times a day",
    "nl": "Haalt tot 300 keer per dag de laatste weergegevens op van het KNMI."
  },
  "category": [
    "climate"
  ],
  "permissions": [
    "homey:manager:geolocation"
  ],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Jonathan van Bochove",
    "email": "bochove@outlook.com"
  },
  "flow": {
    "triggers": [{
      "id": "received-new-data",
      "title": {
        "en": "Received new data from KNMI",
        "nl": "Nieuwe gegevens van het KNMI"
      },
      "hint": {
        "en": "Quite a lot of variables received, current data and forecasts.",
        "nl": "Best veel gegevens worden aangeleverd."
      },
      "tokens": [
        {
          "name": "city",
          "type": "string",
          "title": {
            "en": "City",
            "nl": "Plaats"
          },
          "example": {
            "en": "Location that the provided data is for.",
            "nl": "De plaatsnaam waarop deze gegevens van toepassing zijn."
          }
        },
        {
          "name": "currentTemp",
          "type": "number",
          "title": {
            "en": "Current temperature",
            "nl": "Huidige temperatuur"
          },
          "example": {
            "en": "32.1",
            "nl": "32.1"
          }
        },
        {
          "name": "feelTemp",
          "type": "number",
          "title": {
            "en": "Current feeling temperature",
            "nl": "Huidige gevoels-temperatuur"
          },
          "example": {
            "en": "32.1",
            "nl": "32.1"
          }
        },
        {
          "name": "recap",
          "type":"string",
          "title": {
            "en": "Recap",
            "nl": "Verwachting"
          },
          "example": {
            "en": "Cloudy",
            "nl": "Bewolkt"
          }
        },
        {
          "name": "humidity",
          "type": "number",
          "title": {
            "en": "Humidity",
            "nl": "Luchtvochtigheid"
          },
          "example": {
            "en": "90",
            "nl": "90"
          }
        },
        {
          "name": "windDirection",
          "type": "string",
          "title": {
            "en": "Wind direction",
            "nl": "Windrichting"
          },
          "example": {
            "en": "W",
            "nl": "W"
          }
        },
        {
          "name": "windSpeedMS",
          "type": "number",
          "title": {
            "en": "Wind speed MS",
            "nl": "Windsnelheid MS"
          },
          "example": {
            "en": "2",
            "nl": "2"
          }
        },
        {
          "name": "windForce",
          "type": "number",
          "title": {
            "en": "Wind force",
            "nl": "Windkracht"
          },
          "example": {
            "en": "2",
            "nl": "2"
          }
        },
        {
          "name": "windSpeedKMH",
          "type": "number",
          "title": {
            "en": "Wind speed KMH",
            "nl": "Windsnelheid KMH"
          },
          "example": {
            "en": "10",
            "nl": "10"
          }
        },
        {
          "name": "airPressure",
          "type": "number",
          "title": {
            "en": "Air pressure",
            "nl": "Luchtdruk"
          },
          "example": {
            "en": "1000",
            "nl": "1000"
          }
        },
        {
          "name": "airPressureMMHG",
          "type": "number",
          "title": {
            "en": "Air pressure MMHG",
            "nl": "Luchtdruk MMHG"
          },
          "example": {
            "en": "755",
            "nl": "755"
          }
        },
        {
          "name": "dewPoint",
          "type": "number",
          "title": {
            "en": "Dew point",
            "nl": "Dauwpunt"
          },
          "example": {
            "en": "11",
            "nl": "11"
          }
        },
        {
          "name": "sight",
          "type": "number",
          "title": {
            "en": "Sight",
            "nl": "Zicht"
          },
          "example": {
            "en": "11",
            "nl": "11"
          }
        },
        {
          "name": "expected",
          "type": "string",
          "title": {
            "en": "Expected",
            "nl": "Verwachting"
          },
          "example": {
            "en": "Cloudy",
            "nl": "Bewolkt"
          }
        },
        {
          "name": "sunUp",
          "type": "string",
          "title": {
            "en": "Sun up",
            "nl": "Zonsopgang"
          },
          "example": {
            "en": "06:54",
            "nl": "06:54"
          }
        },
        {
          "name": "sunDown",
          "type": "string",
          "title": {
            "en": "Sun down",
            "nl": "Zonsondergang"
          },
          "example": {
            "en": "21:15",
            "nl": "21:15"
          }
        },
        {
          "name": "expectedTodayRecap",
          "type": "string",
          "title": {
            "en": "Todays expectation",
            "nl": "Verwachting vandaag"
          },
          "example": {
            "en": "Sunny",
            "nl": "Regen"
          }
        },
        {
          "name": "expectedTodayMaxTemp",
          "type": "number",
          "title": {
            "en": "Todays expected max temp",
            "nl": "Verwachte maximale temperatuur"
          },
          "example": {
            "en": "18",
            "nl": "18"
          }
        },
        {
          "name": "expectedTodayMinTemp",
          "type": "number",
          "title": {
            "en": "Todays expected minimum temp",
            "nl": "Verwachte minimale temperatuur"
          },
          "example": {
            "en": "7",
            "nl": "7"
          }
        },
        {
          "name": "expectedTodayWindForce",
          "type": "number",
          "title": {
            "en": "Expected windforce for today",
            "nl": "Verwachte windkracht voor vandaag"
          },
          "example": {
            "en": "2",
            "nl": "2"
          }
        },
        {
          "name": "expectedTodayWindSpeedMS",
          "type": "number",
          "title": {
            "en": "Expected windspeed today MS",
            "nl": "Verwachte windkracht vandaag MS"
          },
          "example": {
            "en": "2",
            "nl": "2"
          }
        },
        {
          "name": "expectedTodayWindSpeedKMH",
          "type": "number",
          "title": {
            "en": "Expected windspeed today KMH",
            "nl": "Verwachte windkracht vandaag KMH"
          },
          "example": {
            "en": "5",
            "nl": "5"
          }
        },
        {
          "name": "expectedTodayWindDirection",
          "type": "string",
          "title": {
            "en": "Expected today wind direction",
            "nl": "Verwachte windrichting vandaag"
          },
          "example": {
            "en": "W",
            "nl": "W"
          }
        },
        {
          "name": "expectedTodayPrecipitation",
          "type": "number",
          "title": {
            "en": "Expected precipitation today",
            "nl": "Verwachte neerslag vandaag"
          },
          "example": {
            "en": "2",
            "nl": "2"
          }
        },
        {
          "name": "expectedTodaySunshine",
          "type": "number",
          "title": {
            "en": "Expected today sunshine",
            "nl": "Verwachte zonkans vandaag"
          },
          "example": {
            "en": "55",
            "nl": "55"
          }
        },
        {
          "name": "expectedTomorrowRecap",
          "type": "string",
          "title": {
            "en": "Expected recap tomorrow",
            "nl": "Verwachting morgen"
          },
          "example": {
            "en": "Rainy",
            "nl": "Bewolkt"
          }
        },
        {
          "name": "expectedTomorrowMaxTemp",
          "type": "number",
          "title": {
            "en": "Expected max temp tomorrow",
            "nl": "Verwachte maximale temperatuur morgen"
          },
          "example": {
            "en": "20",
            "nl": "20"
          }
        },
        {
          "name": "expectedTomorrowMinTemp",
          "type": "number",
          "title": {
            "en": "Expected tomorrow min temp",
            "nl": "Verwachte minimum temperatuur morgen"
          },
          "example": {
            "en": "8",
            "nl": "8"
          }
        },
        {
          "name": "expectedTomorrowWindForce",
          "type": "number",
          "title": {
            "en": "Expected tomorrow wind force",
            "nl": "Verwachte windkracht morgen"
          },
          "example": {
            "en": "2",
            "nl": "2"
          }
        },
        {
          "name": "expectedTomorrowWindSpeedMS",
          "type": "number",
          "title": {
            "en": "Expected tomorrow windspeed MS",
            "nl": "Verwachte windsnelheid morgen MS"
          },
          "example": {
            "en": "5",
            "nl": "5"
          }
        },
        {
          "name": "expectedTomorrowWindSpeedKMH",
          "type": "number",
          "title": {
            "en": "Expected wind speed tomorrow KMH",
            "nl": "Verwachte windsnelheid morgen KMH"
          },
          "example": {
            "en": "10",
            "nl": "10"
          }
        },
        {
          "name": "expectedTomorrowWindDirection",
          "type": "string",
          "title": {
            "en": "Expected wind direction tomorrow",
            "nl": "Verwachte windrichting morgen"
          },
          "example": {
            "en": "W",
            "nl": "W"
          }
        },
        {
          "name": "expectedTomorrowPrecipitation",
          "type": "number",
          "title": {
            "en": "Expected precipitation tomorrow",
            "nl": "Verwachte neerslag morgen"
          },
          "example": {
            "en": "2",
            "nl": "2"
          }
        },
        {
          "name": "expectedTomorrowSunshine",
          "type": "number",
          "title": {
            "en": "Expected sunshine tomorrow",
            "nl": "Verwachte zonkans morgen"
          },
          "example": {
            "en": "55",
            "nl": "55"
          }
        },
        {
          "name": "expectedDayAfterTomorrowRecap",
          "type": "string",
          "title": {
            "en": "expectedDayAfterTomorrowRecap",
            "nl": "expectedDayAfterTomorrowRecap"
          },
          "example": {
            "en": "expectedDayAfterTomorrowRecap",
            "nl": "expectedDayAfterTomorrowRecap"
          }
        },
        {
          "name": "expectedDayAfterTomorrowMaxTemp",
          "type": "number",
          "title": {
            "en": "expectedDayAfterTomorrowMaxTemp",
            "nl": "expectedDayAfterTomorrowMaxTemp"
          },
          "example": {
            "en": "expectedDayAfterTomorrowMaxTemp",
            "nl": "expectedDayAfterTomorrowMaxTemp"
          }
        },
        {
          "name": "expectedDayAfterTomorrowMinTemp",
          "type": "number",
          "title": {
            "en": "expectedDayAfterTomorrowMinTemp",
            "nl": "expectedDayAfterTomorrowMinTemp"
          },
          "example": {
            "en": "expectedDayAfterTomorrowMinTemp",
            "nl": "expectedDayAfterTomorrowMinTemp"
          }
        },
        {
          "name": "expectedDayAfterTomorrowWindForce",
          "type": "number",
          "title": {
            "en": "expectedDayAfterTomorrowWindForce",
            "nl": "expectedDayAfterTomorrowWindForce"
          },
          "example": {
            "en": "expectedDayAfterTomorrowWindForce",
            "nl": "expectedDayAfterTomorrowWindForce"
          }
        },
        {
          "name": "expectedDayAfterTomorrowWindSpeedMS",
          "type": "number",
          "title": {
            "en": "expectedDayAfterTomorrowWindSpeedMS",
            "nl": "expectedDayAfterTomorrowWindSpeedMS"
          },
          "example": {
            "en": "expectedDayAfterTomorrowWindSpeedMS",
            "nl": "expectedDayAfterTomorrowWindSpeedMS"
          }
        },
        {
          "name": "expectedDayAfterTomorrowWindSpeedKMH",
          "type": "number",
          "title": {
            "en": "expectedDayAfterTomorrowWindSpeedKMH",
            "nl": "expectedDayAfterTomorrowWindSpeedKMH"
          },
          "example": {
            "en": "expectedDayAfterTomorrowWindSpeedKMH",
            "nl": "expectedDayAfterTomorrowWindSpeedKMH"
          }
        },
        {
          "name": "expectedDayAfterTomorrowWindDirection",
          "type": "string",
          "title": {
            "en": "expectedDayAfterTomorrowWindDirection",
            "nl": "expectedDayAfterTomorrowWindDirection"
          },
          "example": {
            "en": "expectedDayAfterTomorrowWindDirection",
            "nl": "expectedDayAfterTomorrowWindDirection"
          }
        },
        {
          "name": "expectedDayAfterTomorrowPrecipitation",
          "type": "number",
          "title": {
            "en": "expectedDayAfterTomorrowPrecipitation",
            "nl": "expectedDayAfterTomorrowPrecipitation"
          },
          "example": {
            "en": "expectedDayAfterTomorrowPrecipitation",
            "nl": "expectedDayAfterTomorrowPrecipitation"
          }
        },
        {
          "name": "expectedDayAfterTomorrowSunshine",
          "type": "number",
          "title": {
            "en": "expectedDayAfterTomorrowSunshine",
            "nl": "expectedDayAfterTomorrowSunshine"
          },
          "example": {
            "en": "expectedDayAfterTomorrowSunshine",
            "nl": "expectedDayAfterTomorrowSunshine"
          }
        },
        {
          "name": "currentWeatherAlarm",
          "type": "string",
          "title": {
            "en": "Weather alarm",
            "nl": "Weeralarm"
          },
          "example": {
            "en": "Code orange",
            "nl": "Code oranje"
          }
        }
      ]
    }],
    "actions": [{
      "id": "fetch-new-data",
      "title": {
        "en": "Fetch new data",
        "nl": "Haal nieuwe gegevens"
      }
    }]
  }
}