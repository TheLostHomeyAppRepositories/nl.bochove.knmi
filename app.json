{
  "id": "nl.bochove.knmi",
  "version": "0.1.5",
  "compatibility": ">=5.0.0",
  "sdk": 3,
  "name": {
    "en": "KNMI (Netherlands)",
    "nl": "KNMI (Nederland)"
  },
  "description": {
    "en": "Stay up to date on all weather conditions and forecasts.",
    "nl": "Houd het weer en de vooruitzichten in de gaten."
  },
  "tags": {
    "en": [
      "Climate",
      "Weather",
      "Forecast",
      "Temperature",
      "Netherlands"
    ],
    "nl": [
      "Klimaat",
      "Weer",
      "Weersverwachting",
      "Temperatuur",
      "Nederland"
    ]
  },
  "category": [
    "climate"
  ],
  "permissions": [
    "homey:manager:geolocation"
  ],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Jonathan van Bochove",
    "email": "bochove@outlook.com"
  },
  "homeyCommunityTopicId": 34228,
  "bugs": {
    "url": "https://github.com/bochove/nl.bochove.knmi/issues"
  },
  "flow": {
    "triggers": [
      {
        "id": "received-new-data",
        "title": {
          "en": "Received new data from KNMI",
          "nl": "Nieuwe gegevens van het KNMI"
        },
        "hint": {
          "en": "Quite a lot of variables received, current data and forecasts. All received data can be used in conditional and action cards.",
          "nl": "Alle beschikbare gegevens van het KNMI worden ontsloten in variabelen die gebruikt kunnen worden in conditionele en actie kaarten."
        },
        "tokens": [
          {
            "name": "city",
            "type": "string",
            "title": {
              "en": "City",
              "nl": "Plaats"
            },
            "example": {
              "en": "Location that the provided data is for.",
              "nl": "De plaatsnaam waarop deze gegevens van toepassing zijn."
            }
          },
          {
            "name": "currentTemp",
            "type": "number",
            "title": {
              "en": "Current temperature",
              "nl": "Huidige temperatuur"
            },
            "example": {
              "en": "32.1",
              "nl": "32.1"
            }
          },
          {
            "name": "feelTemp",
            "type": "number",
            "title": {
              "en": "Current feeling temperature",
              "nl": "Huidige gevoels-temperatuur"
            },
            "example": {
              "en": "32.1",
              "nl": "32.1"
            }
          },
          {
            "name": "recap",
            "type": "string",
            "title": {
              "en": "Recap",
              "nl": "Weerbeeld"
            },
            "example": {
              "en": "Cloudy",
              "nl": "Bewolkt"
            }
          },
          {
            "name": "humidity",
            "type": "number",
            "title": {
              "en": "Humidity",
              "nl": "Luchtvochtigheid"
            },
            "example": {
              "en": "90",
              "nl": "90"
            }
          },
          {
